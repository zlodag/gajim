<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="WizardIntelePACSLoginPage" parent="AssistantPage">
    <child>
      <object class="GtkBox">
        <property name="halign">center</property>
        <property name="valign">center</property>
        <property name="orientation">vertical</property>
        <property name="spacing">6</property>
        <child>
          <object class="GajimDropDown" id="_intelepacs_server_dropdown">
            <layout>
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="_intelepacs_username_entry">
            <property name="sensitive">0</property>
            <property name="width-request">300</property>
            <property name="focusable">1</property>
            <property name="tooltip-text" translatable="yes">Username (case sensitive)</property>
            <property name="activates-default">1</property>
            <property name="placeholder-text">Username (case sensitive)</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="_intelepacs_password_entry">
            <property name="sensitive">0</property>
            <property name="focusable">1</property>
            <property name="tooltip-text" translatable="yes">Your password</property>
            <property name="visibility">0</property>
            <property name="activates-default">1</property>
            <property name="placeholder-text" translatable="yes">Password</property>
            <property name="input-purpose">password</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="_intelepacs_log_in_button">
            <property name="label" translatable="yes">_Log In</property>
            <property name="sensitive">0</property>
            <property name="focusable">1</property>
            <property name="tooltip-text" translatable="yes">Log in with your credentials</property>
            <property name="use-underline">1</property>
            <style>
              <class name="suggested-action"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
